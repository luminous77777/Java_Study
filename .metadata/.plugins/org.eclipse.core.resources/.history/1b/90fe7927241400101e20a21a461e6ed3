package lesson01;

public class StirngEx {
	public static void main(String[] args) {
		String name;
		name = "홍길동";
		
		String name2 = "홍길동";
		String name3 = null;
		String name4 = "";
		
		System.out.println("name  :" +name);
		System.out.println("name 2:" +name2);
		System.out.println("name 3:" +name3);
		System.out.println("name 4:" +name4);
		
		System.out.println(1+1);
	}
}
