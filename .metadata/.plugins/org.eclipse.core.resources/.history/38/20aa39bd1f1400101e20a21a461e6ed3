package lesson01;

public class StirngEx {
	public static void main(String[] args) {
		String name;
		name = "홍길동";
		
		String name2 = "홍길동";
		String name3 = null;
		String name4 = "";
		
		System.out.println(name);
		System.out.println(name2);
		System.out.println(name3);
		System.out.println(name4);
	}
}
