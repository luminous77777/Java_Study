package lesson04;

public class OpEx4 {
	public static void main(String[] args) {
		int a= 10;
		int b = 5;
		
		System.out.println(a > b);
		System.out.println(a >= b);
		System.out.println(a < b);
		System.out.println(a <= b);
		System.out.println(a == b);
		System.out.println(a != b);
		
		boolean c = a ==b;
		System.out.println("c : " + c);
		boolean d = c == false;
		System.out.println("d : " + d);
		
		int ko1 = '가';
		int ko2 = '힇';
		System.out.println(ko1);
		System.out.println(ko2);
		System.out.println(ko2-ko1);
	}
}
