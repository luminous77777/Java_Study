package lesson01;

public class HelloWorld {
	
	public static void main(String[] args) {
		
		String name;
		name = "김윤석";
		
		String name2 ="김윤석";
		String name3 = null;
		String name4 = " ";
		
		System.out.println("name2=" + name2);
		System.out.println("name3=" + name3);
		System.out.println("name4=" + name4);
		
		
	}
}